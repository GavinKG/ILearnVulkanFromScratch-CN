
{
    "title" : "Vulkan 学习笔记",
    "author" : "Gavin KG",
	"description" : "Vulkan 学习笔记",
    "language" : "zh-hans",
    "plugins" : [
        "-sharing",
		"github-buttons",
		"expandable-chapters-small",
		"splitter",
		"sitemap-general",
        "page-toc",
        "katex"
    ],
	"pluginsConfig": {
        "github-buttons": {
            "buttons": [{
                "user": "GavinKG",
                "repo": "ILearnVulkanFromScratch-CN",
                "type": "star",
                "size": "small"
              }]
        },
		"sitemap-general": {
			"prefix": "https://gavinkg.github.io/ILearnVulkanFromScratch-CN/"
		},
		"page-toc": {
			"selector": ".markdown-section h1, .markdown-section h2, .markdown-section h3, .markdown-section h4",
			"position": "before-first",
			"showByDefault": true
        }
    }
}